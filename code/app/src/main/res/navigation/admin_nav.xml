<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/admin_nav"
    app:startDestination="@id/adminProfilesFragment">

    <fragment
        android:id="@+id/adminProfilesFragment"
        android:name="com.example.project_part_3.Users.Admin_UI.Admin_profile.Admin_profile_view"
        android:label="Profiles"
        tools:layout="@layout/admin_profiles" >
        <action
            android:id="@+id/action_admin_profile_view_to_adminProfileInfoFragment"
            app:destination="@id/admin_profile_info" />
    </fragment>

    <fragment
        android:id="@+id/adminSearchFragment"
        android:name="com.example.project_part_3.Users.Admin_UI.Admin_search_event_view"
        android:label="Search"
        tools:layout="@layout/admin_search" />

    <fragment
        android:id="@+id/adminNotificationsFragment"
        android:name="com.example.project_part_3.Users.Admin_UI.Admin_notification_view"
        android:label="Notifications"
        tools:layout="@layout/admin_notifications" />
    <fragment android:id="@+id/placeholder" />
</navigation>